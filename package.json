{
  "name": "ukiyojs",
  "version": "3.0.3",
  "description": "Modern parallax library for picture elements and any images",
  "homepage": "https://yitengjun.github.io/ukiyo-js/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yitengjun/ukiyojs.git"
  },
  "main": "./dist/ukiyo.min.js",
  "keywords": [
    "javascript",
    "parallax",
    "scroll",
    "background-image",
    "background",
    "parallax-scrolling"
  ],
  "author": "Yiteng Jun <ito.yitengjun@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "lint": "eslint  \"./src/**/*.js\" --fix",
    "format": "prettier --write \"./src/**/*.js\"",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@rollup/plugin-buble": "^0.21.3",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.2",
    "@semantic-release/npm": "^8.0.3",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.4.1",
    "rollup": "^2.70.1",
    "rollup-plugin-serve": "^2.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "semantic-release": "^18.0.1"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  }
}
